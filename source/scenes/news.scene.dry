// Shared calculations for all news tabs
if (!Q.news_initialized) {
    Q.global_events = "test";
    Q.southern_events = ""; 
    Q.northern_events = "";
    Q.news_initialized = true;
}
if (Q.unemployed) Q.unemployed_2 = Q.unemployed.toFixed(1);
if (Q.north_unemployed) Q.north_unemployed_2 = Q.north_unemployed.toFixed(1);
!}

