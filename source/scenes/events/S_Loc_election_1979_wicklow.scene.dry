on-arrival: {!
// set old results
// election threshold constitutional reform
// banning parties
var new_party_sum = 0;
for (var party of Q.parties) {
    Q['old_' + party + '_loc_wicklow'] = Q[party + '_loc_wicklow'];
    if (Q.constitutional_reform > 0) {
        if (Q[party + '_votes'] < Q.electoral_threshold || (Q.electoral_threshold > 1 && party == 'other') || Q[party + '_south_banned']) {
            Q[party + '_loc_wicklow'] = 0;
        } else {
            Q[party + '_loc_wicklow'] = Q[party + '_votes'];
        }
        new_party_sum += Q[party + '_loc_wicklow'];
    } else {
        Q[party + '_loc_wicklow'] = Q[party + '_votes'];
    }
    // if we use decimals, then we might want to truncate the numbers
    if (Q.use_decimals) {
        Q[party + '_loc_wicklow_disp'] = Q[party+'_loc_wicklow'].toFixed(2);
    }  else {
            Q[party + '_loc_wicklow_disp'] = Q[party+'_loc_wicklow'];
    }
    console.log(party, Q[party + '_loc_wicklow'], Q['old_' + party + '_loc_wicklow']);
    Q['change_' + party + '_loc_wicklow'] = Q[party + '_loc_wicklow'] - Q['old_' + party + '_loc_wicklow'];
    var sign = Q['change_' + party + '_loc_wicklow'] > 0 ? '+'  : '';
    Q['str_change_' + party] = sign + Q['change_' + party + '_loc_wicklow'];//.toFixed(2);
}
// calculate new party sum if electoral reform
if (Q.electoral_threshold && Q.electoral_threshold > 0) {
    for (var party of Q.parties) {
        if (Q[party + '_loc_wicklow'] > 0) {
            if (Q.use_decimals) {
                Q[party + '_loc_wicklow'] = Math.round(Q[party + '_loc_wicklow']*1000/new_party_sum)/10;
            } else {
                Q[party + '_loc_wicklow'] = Math.round(Q[party + '_loc_wicklow']*100/new_party_sum);
            }
        }
        if (Q.use_decimals) {
            Q[party + '_loc_wicklow_disp'] = Q[party+'_loc_wicklow'].toFixed(2);
        } else {
            Q[party + '_loc_wicklow_disp'] = Q[party+'_loc_wicklow'];
        }
        console.log(party, Q[party + '_loc_wicklow'], Q['old_' + party + '_loc_wicklow']);
        Q['change_' + party + '_loc_wicklow'] = Q[party + '_loc_wicklow'] - Q['old_' + party + '_loc_wicklow'];
        var sign = Q['change_' + party + '_loc_wicklow'] > 0 ? '+'  : '';
        Q['str_change_' + party] = sign + Q['change_' + party + '_loc_wicklow'];//.toFixed(2);
    }
}

// Calculate total_l_unsouth_banned by summing _loc_wicklow for all unsouth_banned parties
var total_l_unsouth_banned = 0;
for (var party of Q.parties) {
    if (Q[party + '_south_banned'] != 1) {
        total_l_unsouth_banned += Q[party + '_loc_wicklow'];
    }
}

// Adjust _loc_wicklow values for unsouth_banned parties by dividing by total_l_unsouth_banned and multiplying by 100
for (var party of Q.parties) {
    if (Q[party + '_south_banned'] != 1) {
        Q[party + '_loc_wicklow'] = (Q[party + '_loc_wicklow'] / total_l_unsouth_banned) * 100;
        // Round to 2 decimal places
        Q[party + '_loc_wicklow'] = Math.round(Q[party + '_loc_wicklow'] * 100) / 100;
        if (Q.use_decimals) {
            Q[party + '_loc_wicklow_disp'] = Q[party+'_loc_wicklow'].toFixed(2);
        } else {
            Q[party + '_loc_wicklow_disp'] = Math.round(Q[party+'_loc_wicklow']);
        }
        console.log(party, Q[party + '_loc_wicklow'], Q['old_' + party + '_loc_wicklow']);
        Q['change_' + party + '_loc_wicklow'] = Q[party + '_loc_wicklow'] - Q['old_' + party + '_loc_wicklow'];
        var sign = Q['change_' + party + '_loc_wicklow'] > 0 ? '+' : '';
        Q['str_change_' + party] = sign + Q['change_' + party + '_loc_wicklow'].toFixed(2);
    }
}


var election_result = {'date': new Date(Q.year, Q.month - 1)};
for (var party of Q.parties) {
    election_result[party] = Q[party + '_loc_wicklow'];
}
Q.election_records.push(election_result);
!}
